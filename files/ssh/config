    #ProxyCommand bash -c "ssh etg-redhat@workstation.etgredhat.com.au netcat %h %p" 2> /dev/null 
    # Working proxy command below
    # Source:
    # https://stackoverflow.com/questions/35060642/cant-get-ssh-proxycommand-to-work-ssh-exchange-identification-connection-clos
    ProxyCommand ssh -W %h:%p etg-redhat@workstation.etgredhat.com.au

